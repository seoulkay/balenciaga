package pack1;

public class carUse {
	public static void main(String[] args) {
	car sonata = new car();
	sonata.manufacturerer = "현대";
	sonata.codeName = "소나타";
	sonata.color = "빨간색";
	sonata.buildTime = 200819;
	sonata.motorNumber = 20;
		
	System.out.println(sonata.manufacturer);
  	System.out.println(sonata.codeName);
	System.out.println(sonata.color);
  	System.out.println(sonata.buildTime);
  	System.out.println(sonata.motorNumber);
	
	sonata.start();
	sonata.accelerate();
	sonata.left();
	sonata.decelerate();
	sonata.stop();
	sonata.quit();
	
	}
}
